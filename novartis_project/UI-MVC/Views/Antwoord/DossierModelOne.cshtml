@model JPP.BL.Domain.Antwoorden.DossierAntwoord

@{
    Layout = "~/Views/Shared/_Layout2.cshtml";
    ViewBag.Title = "eigenpagina";
}
<link rel="stylesheet" type="text/css" href="~/Content/DossierModel.css">


<script src="https://maps.googleapis.com/maps/api/js"></script>
<script>
    function initialize() {
        var mapCanvas = document.getElementById('map-canvas');
        var mapOptions = {
            center: new google.maps.LatLng(44.5403, -78.5463),
            zoom: 8,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        }
        var map = new google.maps.Map(mapCanvas, mapOptions)
    }
    google.maps.event.addDomListener(window, 'load', initialize);
</script>

<div id="DossierModelTitle"> <h1>@(Model.titel)</h1></div><!--Hier moet de titel van de pagina komen-->
<div id="DossierModelSubTitle"> <h2>@(Model.subtitel)</h2></div> <!--Hier moet de subtitel(beknopte uitleg) van de pagina komen-->
<div id="DossierModelAfbeelding"> <img src=@(Model.afbeeldingPath) />
    <div id="DossierModelTextVakEen"> <p>@(Model.inhoud)</p>
    
</div><!--Hier moet de eerste textvak van de pagina komen-->
    
    </div><!--Hier moet de afbeelding van de pagina komen-->


<div id="TweeTextvakWrapper">
    <div id="DossierModelTextVakTwee">
        <p>
            @(Model.textvak2)
        </p>
    </div><!--Hier moet de tweedetextvak van de pagina komen-->

    <div id="DossierModelTextVakDrie">
        <p>
            @(Model.textvak3)
        </p>
    </div><!--Hier moet de derde textvak van de pagina komen-->
</div>


<div id="map-canvas"></div>
